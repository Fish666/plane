<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="/src/main/java/dao/IManagerDao.java">
 	<resultMap type="manager" id="managerResults">
 		<id column="id" property="id"/>
 		<result column="name" property="name"/>
 		<result column="pwd" property="pwd"/>
 	</resultMap>
 	
 	<select id="queryUser" parameterType="manager" resultMap="managerResults"> 		
 		select * from manager
 	</select>
</mapper>